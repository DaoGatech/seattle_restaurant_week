{
    "name": "MyExtension",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Gotta catch Youtube events!",
    "permissions": ["tabs", "http://*/*"],
    "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "css": ["core.css"],
      "js": ["core.js","jquery-2.2.3.min.js","jquery-ui.min.js"],
      "run_at": "document_start",
      "all_frames": true
    }
  ]
}